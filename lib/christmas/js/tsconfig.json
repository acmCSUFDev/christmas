{
  "compilerOptions": {
    "target": "es2020",
    "lib": [
      "es6"
    ],
    "module": "es2020",
    "rootDir": "src",
    "resolveJsonModule": true,
    "allowJs": true,
    "outDir": ".",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  }
}
